{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'no-undef': 0,
    'import/no-named-as-default': 0,
    'import/no-named-as-default-member': 0,
    'func-names': 0,
    'max-len': 0,
    'no-console': 0,
    'no-unused-vars': 0,
    'no-multi-assign': 0,
    'comma-dangle': 0,
    'no-floating-decimal': 0,
    'prefer-template': 0,
    'arrow-parens': 0,
    'import/no-cycle': 0,
    'eol-last': 0,
  }
}
